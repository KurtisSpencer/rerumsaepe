{
  "name": "rerumsaepe",
  "version": "1.0.0",
  "description": "Libsodium for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "typedocMain": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/KurtisSpencer/rerumsaepe.git"
  },
  "author": "SigurdGutmann",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "napi": {
    "name": "sodium",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@jsdevtools/npm-publish": "^1.4.3",
    "@napi-rs/cli": "^2.5.0",
    "@sapphire/eslint-config": "^4.3.2",
    "@sapphire/prettier-config": "^1.4.1",
    "@sapphire/ts-config": "^3.3.3",
    "@swc-node/register": "^1.4.2",
    "@types/eslint": "^8",
    "@types/jest": "^27.4.1",
    "@types/libsodium-wrappers": "^0",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "benny": "^3.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-esm-wrapper": "^1.1.3",
    "libsodium-wrappers": "^0.7.10",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "semver-regex": "^4.0.2",
    "tweetnacl": "^1.0.3",
    "typedoc": "^0.22.13",
    "typedoc-plugin-extras": "^2.2.3",
    "typedoc-plugin-mdn-links": "^1.0.5",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "keywords": [
    "sodium",
    "incidunt",
    "typescript",
    "ts",
    "et",
    "napi",
    "ex",
    "ullam",
    "assumenda"
  ],
  "bugs": {
    "url": "https://github.com/KurtisSpencer/rerumsaepe.git/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@3.2.0",
  "prettier": "@sapphire/prettier-config"
}